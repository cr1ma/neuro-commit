{
  "name": "neuro-commit",
  "version": "0.1.2",
  "description": "neuro-commit CLI utility",
  "author": "cr1ma.dev",
  "bin": {
    "neuro-commit": "bin/neuro-commit.js"
  },
  "scripts": {
