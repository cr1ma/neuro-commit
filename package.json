  },
  "files": [
    "bin/",
    "src/"
  ],
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "eslint": "^10.0.0",
    "globals": "^17.3.0"
  },
  "dependencies": {
    "tiktoken": "^1.0.22",
    "update-notifier": "^7.3.1"
